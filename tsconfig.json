    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": false,
    "sourceMap": false,
    "removeComments": false,
    "outDir": "lib",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
